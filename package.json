{
	"name": "nmanager",
	"version": "1.4.0",
	"description": "A network manager cli",
	"main": "dist/index.js",
	"type": "module",
	"homepage": "https://github.com/SirMarkus73/network-manager",
	"repository": {
		"type": "git",
		"url": "https://github.com/SirMarkus73/network-manager"
	},
	"scripts": {
		"build": "tsc && tsc-alias",
		"commit": "cz",
		"dev": "pnpm run build && pnpm run start",
		"start": "node dist/index.js",
		"format": "biome format --write",
		"lint": "biome lint && tsc --noEmit",
		"lint:fix": "biome lint --fix",
		"ci:biome": "biome ci",
		"ci:tsc": "tsc --noEmit",
		"ci": "pnpm run ci:biome && pnpm run ci:tsc"
	},
	"keywords": [
		"cli",
		"network",
		"manager"
	],
	"bin": {
		"nmanager": "dist/index.js"
	},
	"author": "Marcos Barrero",
	"license": "MIT",
	"packageManager": "pnpm@10.20.0",
	"dependencies": {
		"@inquirer/prompts": "^7.9.0",
		"ansi-escapes": "^7.1.1",
		"chalk": "^5.6.2",
		"cli-table3": "^0.6.5",
		"commander": "^14.0.1",
		"nanospinner": "^1.2.2",
		"table": "^6.9.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@commander-js/extra-typings": "^14.0.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/node": "^22.7.5",
		"cz-conventional-changelog": "^3.3.0",
		"semantic-release": "^25.0.1",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.6.3"
	},
	"files": [
		"dist",
		"README.MD",
		"LICENSE",
		"CHANGELOG.md"
	],
	"publishConfig": {
		"access": "public"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"pnpm": {
		"overrides": {
			"tmp": "^0.2.4"
		}
	}
}
