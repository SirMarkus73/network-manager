{
	"name": "nmanager",
	"version": "1.0.1",
	"description": "A network manager cli",
	"main": "dist/index.js",
	"type": "module",
	"homepage": "https://github.com/SirMarkus73/network-manager/",
	"repository": {
		"type": "git",
		"url": "https://github.com/SirMarkus73/network-manager/"
	},
	"scripts": {
		"build": "tsc && tsc-alias",
		"dev": "pnpm run build && pnpm run start",
		"start": "node dist/index.js",
		"format": "biome format --write",
		"lint": "biome lint && tsc --noEmit",
		"lint:fix": "biome lint --fix",
		"ci:biome": "biome ci",
		"ci:tsc": "tsc --noEmit",
		"ci": "pnpm run ci:biome && pnpm run ci:tsc"
	},
	"keywords": [
		"cli",
		"network",
		"manager"
	],
	"bin": {
		"nmanager": "dist/index.js"
	},
	"author": "Marcos Barrero",
	"license": "MIT",
	"packageManager": "pnpm@10.18.3",
	"dependencies": {
		"@inquirer/prompts": "^7.9.0",
		"chalk": "^5.6.2",
		"cli-table3": "^0.6.5",
		"commander": "^14.0.1",
		"nanospinner": "^1.2.2",
		"table": "^6.9.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@commander-js/extra-typings": "^14.0.0",
		"@types/node": "^22.7.5",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.6.3"
	}
}
