{
	"name": "nmanager",
	"version": "0.0.1",
	"description": "A network manager cli",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"start": "node dist/index.js",
		"format": "biome format --write",
		"lint": "biome lint && tsc --noEmit"
	},
	"keywords": [
		"cli",
		"network",
		"manager"
	],
	"bin": {
		"nmanager": "dist/index.js"
	},
	"author": "Marcos Barrero",
	"license": "MIT",
	"packageManager": "pnpm@10.18.3",
	"dependencies": {
		"commander": "^14.0.1"
	},
	"devDependencies": {
		"typescript": "^5.6.3",
		"@types/node": "^22.7.5",
		"@biomejs/biome": "2.2.6"
	}
}
