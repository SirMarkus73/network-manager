{
	"name": "nmanager",
	"version": "0.1.0",
	"description": "A network manager cli",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc && tsc-alias",
		"dev": "tsc --watch",
		"start": "node dist/index.js",
		"format": "biome format --write",
		"lint": "biome lint && tsc --noEmit",
		"ci:biome": "biome ci",
		"ci:tsc": "tsc --noEmit",
		"ci": "pnpm run ci:biome && pnpm run ci:tsc"
	},
	"keywords": [
		"cli",
		"network",
		"manager"
	],
	"bin": {
		"nmanager": "dist/index.js"
	},
	"author": "Marcos Barrero",
	"license": "MIT",
	"packageManager": "pnpm@10.18.3",
	"dependencies": {
		"@inquirer/prompts": "^7.9.0",
		"commander": "^14.0.1",
		"nanospinner": "^1.2.2"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@commander-js/extra-typings": "^14.0.0",
		"@types/node": "^22.7.5",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.6.3"
	}
}
